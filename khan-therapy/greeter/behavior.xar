                                                                                                            <bitmap>media/images/box/box-python-script.png</bitmap>
                                                                                                            <script language="4">
                                                                                                                <content>
                                                                                                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.motion = ALProxy("ALMotion")

        self.outs = [self.out01, self.out02, self.out03, self.out04, self.out05, self.out06, self.out07, self.out08, self.out09]
        self.choice = []

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):

        # re-enable hand stiffness
        self.motion.setStiffnesses(["LHand", "RHand"], 1.0)

        if not len(self.choice):
            self.choice = self.outs[:]
            random.shuffle(self.choice)

        self.choice.pop()()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped]]>
</content>
