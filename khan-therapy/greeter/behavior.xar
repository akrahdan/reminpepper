                                                                    <script language="4">
                                                                        <content>
                                                                            <![CDATA[import time
import random

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.canceled = False
        self.is_playing = False

    def onUnload(self):
        pass

    def onInput_onStart(self):
        self.playAnim()

    def onInput_cancel(self):
        self.canceled = True
        if not self.is_playing:
            self.onStopped()

    def onInput_playDone(self):
        self.is_playing = False
        if self.canceled:
            self.onStopped()
        else:
            time.sleep(random.randint(self.getParameter("waitMin"), self.getParameter("waitMax")))
            self.playAnim()

    def playAnim(self):
        self.is_playing = True
        self.play()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
